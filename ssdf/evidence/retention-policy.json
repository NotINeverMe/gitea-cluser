{
  "lifecycle": {
    "rule": [
      {
        "action": {
          "type": "SetStorageClass",
          "storageClass": "COLDLINE"
        },
        "condition": {
          "age": 90,
          "matchesPrefix": ["evidence/"],
          "matchesSuffix": [".tar.gz"]
        },
        "description": "Move evidence packages to COLDLINE storage after 90 days"
      },
      {
        "action": {
          "type": "SetStorageClass",
          "storageClass": "ARCHIVE"
        },
        "condition": {
          "age": 365,
          "matchesPrefix": ["evidence/"],
          "matchesSuffix": [".tar.gz"]
        },
        "description": "Move evidence packages to ARCHIVE storage after 1 year"
      },
      {
        "action": {
          "type": "Delete"
        },
        "condition": {
          "age": 2555,
          "matchesPrefix": ["evidence/"]
        },
        "description": "Delete evidence packages after 7 years (2555 days) retention period"
      },
      {
        "action": {
          "type": "AbortIncompleteMultipartUpload"
        },
        "condition": {
          "age": 7
        },
        "description": "Clean up incomplete uploads after 7 days"
      }
    ]
  },
  "versioning": {
    "enabled": false
  },
  "logging": {
    "logBucket": "compliance-evidence-ssdf-logs",
    "logObjectPrefix": "access-logs/"
  },
  "retentionPolicy": {
    "retentionPeriod": 220752000,
    "isLocked": false,
    "effectiveTime": "2025-10-07T00:00:00Z"
  },
  "iamConfiguration": {
    "uniformBucketLevelAccess": {
      "enabled": true,
      "lockedTime": "2025-10-07T00:00:00Z"
    }
  },
  "encryption": {
    "defaultKmsKeyName": "projects/PROJECT_ID/locations/LOCATION/keyRings/KEYRING/cryptoKeys/KEY"
  },
  "labels": {
    "purpose": "compliance-evidence",
    "framework": "ssdf",
    "retention": "7-years",
    "environment": "production"
  },
  "cors": [],
  "website": {},
  "storageClass": "STANDARD",
  "location": "US",
  "locationType": "multi-region"
}
